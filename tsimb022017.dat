program MenorMaior ;

var
	int i, n, num, menor, maior;

{
	read (n, i, menor);
	i = 1;
	while (i <= n) {
		read (num);
		if (i == 1) {
			menor = num;
			maior = num;
		}
		else {
			if (num > maior) maior = num;
			if (num < menor) menor = num;
		}
		i = i + 1;
	}
	write ("Maior = ", maior, "\nMenor = ", menor);
    if (i+3) i = i-5;
    while (i * 5) i = i / 4;
}
